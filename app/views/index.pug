extends template

block content
    h1 Welcome to Crimson Delivery
    form#login-form
        h2 Sign up for HSA Dorm Supply Deliveries
        input(type='text' name='name' placeholder='Name') 
        br
        input(type='email' name='email' placeholder='Email') 
        br
        input(type='number' name='classYear' placeholder='Class year') 
        br
        input(type='phone' name='phone' placeholder='Phone Number') 
        br

        button(type='submit' onclick ="testSubmit()") Submit


